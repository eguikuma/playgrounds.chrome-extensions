{
  "name": "gmail-shield",
  "description": "Gmailのプライバシーを保護するシールド機能を提供します",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf distributions",
    "package:deps": "ncu",
    "package:update": "ncu -i",
    "package:update:force": "ncu -u",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --check \"**/*.{ts,js,css,html,md,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,js,css,html,md,json}\"",
    "tsc": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ci": "npm-run-all prettier:fix eslint:fix tsc test clean build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/chrome": "^0.1.31",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.13",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "lodash": "^4.17.21",
    "npm-check-updates": "^19.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.13"
  },
  "license": "MIT"
}
